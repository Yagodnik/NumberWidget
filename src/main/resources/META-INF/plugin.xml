<idea-plugin>
    <id>com.example.demo</id>

    <name>Ineditor Number Converter</name>

    <vendor email="YagodnikDev@yandex.ru" url="https://github.com/Yagodnik/NumberWidget/">Yagodnik</vendor>

    <description>Small widget which will help to easily convert numbers during coding</description>

    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <editorFactoryListener implementation="com.yagodnik.numbertool.PluginEditorFactoryListener" />
    </extensions>
</idea-plugin>